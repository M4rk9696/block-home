{
    "manifest_version": 2,
    "name": "Home Blocks",
    "version": "0.4.8",
    "description": "Custom home page!",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "icons": {
      "16": "homeblock16.png",
      "48": "homeblock48.png",
      "128": "homeblock128.png"
    },
    "permissions": ["storage", "notifications"]
}
